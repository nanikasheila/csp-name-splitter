{
  "$schema": "./settings.schema.json",
  "github": {
    "owner": "nanikasheila",
    "repo": "csp-name-splitter",
    "mergeMethod": "merge"
  },
  "issueTracker": {
    "provider": "none"
  },
  "branch": {
    "user": "nanikasheila",
    "format": "<user>/<type>-<description>"
  },
  "project": {
    "name": "csp-name-splitter",
    "language": "python",
    "entryPoint": "name_splitter.app.cli:main",
    "test": {
      "command": "python -m pytest tests/",
      "directory": "tests/",
      "pattern": "test_*.py"
    }
  },
  "agents": {
    "model": "Claude Sonnet 4.6 (copilot)",
    "developer": {},
    "reviewer": {},
    "writer": {},
    "manager": {},
    "architect": {},
    "assessor": {}
  }
}
